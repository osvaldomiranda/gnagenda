= javascript_include_tag "//www.google.com/jsapi", "chartkick" 

.container{:role => "main"}
  .col-md-12
    .panel.panel-default
      .panel-heading
        %h3.panel-title Prueba de Grafico
      .panel-body

        %h3 Grafico
   
        .row
          .col-md-12.text-center
            = pie_chart @chart_data

        .row
          .col-md-12.text-center
            = column_chart @chart_data
